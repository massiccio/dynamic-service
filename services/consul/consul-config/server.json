{
    "recursors": [
      "1.1.1.1",
      "9.9.9.9"
    ],
    "dns_config": {
      "recursor_strategy": "random"
    },
    "ports": {
      "dns": 53
    },
    "services": [
      {
        "name": "redis",
        "port": 6379,
        "tags": ["cache", "database"],
        "address": "redis",
        "check": {
          "tcp": "redis:6379",
          "interval": "10s",
          "timeout": "5s"
        }
      },
      {
        "name": "krakend",
        "port": 8080,
        "tags": ["api-gateway"],
        "address": "krakend",
        "check": {
          "tcp": "krakend:8080",
          "interval": "10s",
          "timeout": "5s"
        }
      },
      {
        "name": "caddy",
        "port": 443,
        "tags": ["ssl"],
        "address": "caddy",
        "check": {
          "tcp": "caddy:443",
          "interval": "10s",
          "timeout": "5s"
        }
      }
    ]
  }